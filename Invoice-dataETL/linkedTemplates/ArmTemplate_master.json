{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Invoice-dataETL"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"InvoiceETL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'InvoiceETL'"
		},
		"Invoicelinkservices_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Invoicelinkservices'"
		},
		"InvoiceETL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://invoicedataetl.dfs.core.windows.net/"
		},
		"Invoicelinkservices_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://invoicedataetl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Invoice-dataETL_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"InvoiceETL_accountKey": {
						"value": "[parameters('InvoiceETL_accountKey')]"
					},
					"Invoicelinkservices_accountKey": {
						"value": "[parameters('Invoicelinkservices_accountKey')]"
					},
					"InvoiceETL_properties_typeProperties_url": {
						"value": "[parameters('InvoiceETL_properties_typeProperties_url')]"
					},
					"Invoicelinkservices_properties_typeProperties_url": {
						"value": "[parameters('Invoicelinkservices_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Invoice-dataETL_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Invoice-dataETL_ArmTemplate_0')]"
			]
		}
	]
}