{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Invoice_Table_ETL",
						"type": "DatasetReference"
					},
					"name": "InvoiceTableETL"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Invoice_Table_ET",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "Invoicelinkservices",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {Invoice_No.} as string,",
				"          {Corporation_No.} as string,",
				"          {Company_No.} as string,",
				"          INVC_TYPE as string,",
				"          {Invoice_Confirmation_No.} as date,",
				"          Invoice_Date as date,",
				"          {Order_No.} as string,",
				"          Lead_No as string,",
				"          {\"Delivered/Not Delivered Flag\"} as string,",
				"          Flag as string,",
				"          { \"Invoice_Type Name\"} as string,",
				"          Order_Date as date,",
				"          Model_Code as string,",
				"          TRIM_Code as string,",
				"          ORD_EXPCT_DLVRY_DATE as date,",
				"          Order_Amt as float,",
				"          ORD_Cncl_Date as date,",
				"          ORD_BILL_TO_STATE as string,",
				"          ORD_BILL_TO_CITY as string,",
				"          TEAM_LEADER as string,",
				"          {Trader No.} as string,",
				"          Team_Leader_Name as string,",
				"          Team_Leader_Code as string,",
				"          {\"Active/Inactive\"} as string,",
				"          link_date as date,",
				"          Lead_STAT as string,",
				"          Lead_TYPE as string,",
				"          Lead_Sub_Source_Name as string,",
				"          Lead_Source as string,",
				"          Lead_Status as string,",
				"          Lead_Category_Type as string,",
				"          Lead_QTY as integer,",
				"          Delivery_Date as string,",
				"          Lead_Source_Type as string,",
				"          ORD_HYPO_NAME as string,",
				"          Price as float,",
				"          INVC_CRTE_DTIME as timestamp,",
				"          INVC_CRTE_EMP_NO as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> InvoiceTableETL",
				"InvoiceTableETL sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['InvoiceTableETL.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}